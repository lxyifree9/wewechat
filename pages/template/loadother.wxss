@import "product.wxss";
/**content**/
.content{
  width: 100%;
  display: flex;
  flex-direction: column;
}
/**contNav**/
.contNav{
  width: 100%;
  height: 320rpx;
  overflow: hidden;
  background-image: -webkit-linear-gradient(top, rgb(255,255,255), rgb(220,220,220));
  display: block;
  padding-bottom: 20rpx;
}
.conNavList{
  width: 25%;
  text-align: center;
  display: inline-block;
}
.conNavListImg{
  display: block;
  margin: 5% 20%;
  width: 60%;
  height: 112rpx;
}
.conNavListTxt{
  display: block;
  font-size: 24rpx;
  color: #191970;
  margin: 0 15%;
  width: 70%;
  height: 30rpx;
  overflow: hidden;
}
